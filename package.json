{
  "name": "simple-cfn",
  "description": "Simple CloudFormation wrapper for Node.js ",
  "version": "0.2.0",
  "author": "Paul Sladek @pbsladek",
  "bin": {
    "simple-cfn": "./bin/run"
  },
  "bugs": "https://github.com/pbsladek/simple-cfn/issues",
  "dependencies": {
    "@aws-sdk/client-cloudformation-node": "^0.1.0-preview.2",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@types/js-yaml": "^3.12.2",
    "js-yaml": "^3.13.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.15",
    "aws-sdk": "^2.624.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/pbsladek/simple-cfn",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "simple-cfn",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "update": {
      "s3": {
        "bucket": "pbsladek-simple-cfn",
        "acl": "private"
      },
      "updateConfig": {
        "node": {
          "targets": "linux-arm,linux-armv7l,linux-x64,linux-s390x,darwin-x64"
        }
      }
    }
  },
  "repository": "pbsladek/simple-cfn",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "dist": "oclif-dev pack --xz -t linux-arm,linux-armv7l,linux-x64,linux-s390x,darwin-x64",
    "pub": "oclif-dev publish",
    "version": "oclif-dev readme"
  },
  "types": "lib/index.d.ts"
}
